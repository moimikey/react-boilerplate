{
  "presets": [
    ["latest", {
      "es2015": {
        "spec": false
      }
    }],
    "stage-0",
    "react"
  ],
  "plugins": [
    "ramda",
    "transform-decorators-legacy",
    "transform-decorators",
    ["transform-runtime", {
      "polyfill": true
    }]
  ],
  "env": {
    "development": {
      "plugins": [
        "react-hot-loader/babel"
      ]
    },
    "production": {},
    "test": {}
  }
}
