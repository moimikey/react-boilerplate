{
  "presets": [
    "es2015",
    "es2016",
    "es2017",
    "stage-0",
    "react"
  ],
  "plugins": [
    ["transform-runtime", {
      "polyfill": true
    }],
    "ramda",
    "transform-decorators-legacy",
    "transform-decorators"
  ],
  "env": {
    "production": {},
    "development": {
      "plugins": [
        "react-hot-loader/babel"
      ]
    }
  }
}
