{
  "presets": [
    ["latest", {
      "es2015": {
        "spec": true,
        "strict": false
      }
    }],
    "stage-0",
    "react"
  ],
  "plugins": [
    "ramda",
    "transform-decorators-legacy",
    "transform-decorators",
    "transform-react-constant-elements",
    "transform-react-pure-class-to-function"
  ],
  "env": {
    "development": {
      "plugins": [
        "react-hot-loader/babel"
      ]
    },
    "production": {
      "plugins": [
        "transform-react-remove-prop-types",
        ["transform-runtime", {
          "polyfill": true,
          "regenerator": true
        }]
      ]
    },
    "test": {}
  }
}
