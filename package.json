  "scripts": {
    "build": "cross-env webpack --env config/index.js",
    "build:production": "build",
    "clean": "rimraf .eslintcache dist/**/* *.log*",
    "clean:all": "npm-run-all --sequential clean && rimraf node_modules",
    "config": "babel-node start.js",
    "lint": "eslint --cache src",
    "health": "node healthcheck.js",
    "prebuild": "npm-run-all --sequential lint clean",
    "server:production": "cross-env NODE_ENV=production REDIS=redis://user:password@hostname:port/db DEBUG=* PROD_SERVER_HOST=0.0.0.0 PROD_SERVER_HOST=8080 koa-cluster server.production.js",
    "start": "npm-run-all start:development",
    "start:chrome": "sleep 10 && /Applications/Google\\ Chrome.app/Contents/MacOS/Google\\ Chrome --flag-switches-begin --enable-devtools-experiments --enable-precise-memory-info --enable-memory-info --new-window --user-data-dir=/tmp/.chrome-instance-tmp --flag-switches-end http://vcap.me:3000",
    "start:development": "cross-env NODE_ENV=development REDIS_HOST=redis-18658.c8.us-east-1-3.ec2.cloud.redislabs.com REDIS_PORT=18658 DEBUG=*,-babel*,-eslint*,-koa*,-css-modules*,-engine*,-socket.io* DEV_SERVER_HOST=vcap.me DEV_SERVER_PORT=3000 nodemon -w config --exec node --abort_on_uncaught_exception bootstrap.js",
    "stat": "cross-env NODE_ENV=development webpack --env development --profile --json > stats.json",
    "test": "cross-env NODE_PATH=./src NODE_ENV=test babel-tap --bail --no-color --reporter=spec src/**/*.spec.js",
    "test:dev": "cross-env NODE_PATH=./src NODE_ENV=test babel-tap src/**/*.spec.js",
    "test:watch": "cross-env NODE_PATH=./src NODE_ENV=test nodemon -w src --exec npm run test:dev"
  },
